algoritmo aula03_ex02
	inicio
	   real ladoA, ladoB
	   escreva ("Digite lado A em cm")
	   leia (lado A)
	   escreva ("Digite lado B em cm")
	   leia (ladoB)
	   calcular(ladoA, ladoB)
	fim
		
void calcular(ladoA, ladoB)
	inicio
	  real area
	  area=(ladoA * ladoB)
	  escreva ("O valor da area é:" + area + "cm²")
	fim
		