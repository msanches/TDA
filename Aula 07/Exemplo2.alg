algoritmo exemplo2 
   caractere msg
   inteiro n
    início
    escreva ("Digite um número: ")
    leia (n)
    escreva ("Digite um texto: ")
    leia (msg)
    exibirMensagem(msg,n)
    fim

void exibirMensagem(caractere texto, inteiro num)
    inicio
        escreva ("Mensagem: " + texto + " número"+ num)
    fim

