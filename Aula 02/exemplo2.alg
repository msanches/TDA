algoritmo Carros
	início
	real valor, km
	literal modelo
			escreva ("Digite o modelo do carro")
			leia (modelo)
			escreva ("Digite  a quilometragem do carro")
			leia (km)
			escreva ("Digite  o valor do carro")
			leia (valor)
			escreva ("Os dados do carro são: Modelo =  "+ modelo + 
						"Quilometragem = "+ km + "Valor = "+ valor)
	fim